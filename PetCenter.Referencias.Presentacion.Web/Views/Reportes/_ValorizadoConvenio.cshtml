@{
    ViewBag.TituloMenu = "Valorizado por Convenio";
    Layout = "~/Views/Comun/Inicio/_Estructura.cshtml";
}


<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.css">
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.min.js"></script>


<fieldset class="fieldset-filter">
    <legend class="text-muted h5">Ranking Covenios</legend>
    <div id="graficoValorizado" style="height: 250px;"></div>
</fieldset>

<script>
    $(document).ready(function () {
        $.get('@Url.Action("ObtenerTotalConvenio")', function (result) {
            Morris.Bar({
                element: 'graficoValorizado',
                data: result,
                xkey: 'NombreConvenio',
                ykeys: ['Total'],
                labels: ['Total']
            });
        });
    });
</script>