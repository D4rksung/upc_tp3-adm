@using PetCenter.Referencias.Presentacion.Web.Resources
@using PetCenter.Referencias.Presentacion.Web.Helpers.Html
@model PetCenter.Referencias.Presentacion.Web.Models.Registros.Evaluacion.EvaluacionEditorModelo

<fieldset class="fieldset-filter">
    <legend class="text-muted h5">Evaluar clínica Referencia</legend>

    <div class="asistencia-group">
        <div class="col-md-10">
            <table class="table table-striped table-bordered table-condensed asistencia-table-grid">
                <tr>
                    <td class='asistencia-column-left col-sm-2'>Recibo de Servicios</td>
                    <td class='asistencia-column-center col-sm-6'>

                        @if (Model.Solicitud.DocReciboObjeto != null)
                        {
                        <a class="control-label asistencia-puntero" onclick="archivojs.descargaArchivo('@Url.Action("Descarga", "Evaluacion", new { idSolicitud = Model.Solicitud.NroSolicitud, identificador = 1 })','#formEvaluacion')">
                            @Model.Solicitud.DocReciboTitulo
                        </a>
                        }

                    </td>
                </tr>
                <tr>
                    <td class='asistencia-column-left col-sm-2'>Constancia colegiatura veterinario</td>
                    <td class='asistencia-column-center col-sm-6'>
                        @if (Model.Solicitud.DocColegiaturaObjeto != null)
                        {
                        <a class="control-label asistencia-puntero" onclick="archivojs.descargaArchivo('@Url.Action("Descarga", "Evaluacion", new { idSolicitud = Model.Solicitud.NroSolicitud, identificador = 2 })','#formEvaluacion')">
                            @Model.Solicitud.DocColegiaturaTitulo
                        </a>
                        }

                    </td>
                </tr>
                <tr>
                    <td class='asistencia-column-left col-sm-2'>Reporte SUNAT</td>
                    <td class='asistencia-column-center col-sm-6'>
                        @if (Model.Solicitud.DocSunatObjeto != null)
                        {
                        <a class="control-label asistencia-puntero" onclick="archivojs.descargaArchivo('@Url.Action("Descarga", "Evaluacion", new { idSolicitud = Model.Solicitud.NroSolicitud, identificador = 3 })','#formEvaluacion')">
                            @Model.Solicitud.DocSunatTitulo
                        </a>
                        }

                    </td>
                </tr>
                <tr>
                    <td class='asistencia-column-left col-sm-2'>Licencia funcionamiento</td>
                    <td class='asistencia-column-center col-sm-6'>
                        @if (Model.Solicitud.DocLicenciaObjeto != null)
                        {
                        <a class="control-label asistencia-puntero" onclick="archivojs.descargaArchivo('@Url.Action("Descarga", "Evaluacion", new { idSolicitud = Model.Solicitud.NroSolicitud, identificador = 4 })','#formEvaluacion')">
                            @Model.Solicitud.DocLicenciaTitulo
                        </a>
                        }

                    </td>
                </tr>
                <tr>
                    <td class='asistencia-column-left col-sm-2'>Reporte de central de riesgo</td>
                    <td class='asistencia-column-center col-sm-6'>
                        @if (Model.Solicitud.DocCentralObjeto != null)
                        {
                        <a class="control-label asistencia-puntero" onclick="archivojs.descargaArchivo('@Url.Action("Descarga", "Evaluacion", new { idSolicitud = Model.Solicitud.NroSolicitud, identificador = 5 })','#formEvaluacion')">
                            @Model.Solicitud.DocCentralTitulo
                        </a>
                        }

                    </td>
                </tr>

            </table>
        </div>
        <div class="col-md-2 text-center">
             <br />
            <div class="form-group asistencia-group">
                <a href="http://www.sunat.gob.pe" target="_blank" title="Sunat">
                    <img src="~/Resources/Sitio/imgs/sunat.jpg" alt="Sunat" />
                </a>
            </div>
            <div class="form-group asistencia-group">
                <a href="http://www.cmvl.pe" target="_blank" title="CMVL">
                    <img src="~/Resources/Sitio/imgs/cmvl.jpg" alt="CMVL" />
                </a>
            </div>
            <div class="form-group asistencia-group">
                <a href="https://www.infocorp.com.pe/login_equifax.asp" target="_blank" title="Equifax">
                    <img src="~/Resources/Sitio/imgs/equifax.png" alt="Equifax" />
                </a>

            </div>
        </div>
    </div>


</fieldset>

<script type="text/javascript" charset="utf-8">
    $(document).ready(function () {

    });
</script>
